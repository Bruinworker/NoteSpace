{
  "name": "notespace",
  "version": "1.0.0",
  "description": "NoteSpace - Collaborative note-taking platform",
"scripts": {
  "start": "concurrently \"npm run backend\" \"npm run frontend\"",
  "backend": "py -m backend.app",
  "frontend": "cd frontend && npm start",
  "install-all": "py -m pip install -r requirements.txt && cd frontend && npm install"
},
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

